{
  "name": "pullquester",
  "version": "0.2.1",
  "description": "Issues a pull request using configuration files in your current repo.",
  "author": "Jonathan Park <jpark@daptiv.com>",
  "keywords": [
    "github",
    "manager",
    "pull",
    "request"
  ],
  "license": "BSD-2-Clause",
  "dependencies": {
    "inquirer": "~0.4.1",
    "lodash": "~2.4.1",
    "github": "~0.1.12",
    "q": "~1.0.0",
    "commander": "~2.1.0",
    "temp": "~0.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daptiv/PullQuester"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "bugs": {
    "url": "http://github.com/daptiv/PullQuester/issues"
  },
  "preferGlobal": true,
  "bin": {
    "pull": "./bin/cli.js"
  }
}
